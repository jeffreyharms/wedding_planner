@model Wedding

<h1>Plan Your Wedding</h1>
<div class="container">
    <div class="row">
        <div class="col">
            <form asp-action="NewWedding" asp-controller="Wedding" method="POST">
                <input type="hidden" asp-for="UserId" value=@Context.Session.GetInt32("UUID")>
                <label asp-for="WedderOne"></label>
                <input asp-for="WedderOne">
                <label asp-for="WedderTwo"></label>
                <input asp-for="WedderTwo">
                <label asp-for="Date"></label>
                <input asp-for="Date">
                <label asp-for="Address"></label>
                <input asp-for="Address">
                <button>Create</button>
            </form>
        </div>
    </div>
</div>